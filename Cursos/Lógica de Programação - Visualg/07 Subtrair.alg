algoritmo "Subtrair"
// Função :
// Autor :
// Data : 17/11/2010
// Seção de Declarações 
var
   numero1 : inteiro
   numero2 : inteiro
   final : inteiro
inicio
// Seção de Comandos 
   escreval ("Entre com o primeiro número")
   leia (numero1)
   escreval ("Entre com o segundo número")
   leia (numero2)
   
   final <- numero1 - numero2
   escreval ("Resultado final ", final)

fimalgoritmo
